{
  "name": "elite-designer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "server": "node server/app.js",
    "server:dev": "nodemon server/app.js",
    "server:mock": "nodemon server/app-mock.js",
    "create-admin": "node server/create-admin.js",
    "start:all": "concurrently \"npm run server:mock\" \"npm run dev\"",
    "stop:all": "taskkill /F /IM node.exe",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@libsql/client": "^0.17.0",
    "@vueuse/core": "^10.9.0",
    "aos": "^2.3.4",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.2",
    "pinia": "^2.1.7",
    "three": "^0.179.1",
    "vue": "^3.5.18",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@types/aos": "^3.0.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^24.3.0",
    "@types/three": "^0.179.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "gh-pages": "^6.3.0",
    "nodemon": "^3.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vue-tsc": "^3.0.5"
  }
}
